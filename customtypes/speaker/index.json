{
  "format": "custom",
  "id": "speaker",
  "label": "Speaker",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug",
          "placeholder": "URL slug"
        }
      },
      "name": {
        "type": "StructuredText",
        "config": {
          "label": "Name",
          "placeholder": "Speaker's name",
          "allowTargetBlank": true,
          "multi": "heading1"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {
            "width": 300,
            "height": 400
          },
          "thumbnails": []
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "Role",
          "allowTargetBlank": true,
          "multi": "heading4"
        }
      },
      "company": {
        "type": "StructuredText",
        "config": {
          "label": "Company",
          "placeholder": "Company",
          "allowTargetBlank": true,
          "multi": "heading4"
        }
      },
      "bio": {
        "type": "StructuredText",
        "config": {
          "label": "Bio",
          "placeholder": "Bio",
          "allowTargetBlank": true,
          "multi": "paragraph"
        }
      },
      "twitter": {
        "type": "Link",
        "config": {
          "label": "Twitter",
          "placeholder": "Twitter profile URL",
          "allowTargetBlank": true,
          "select": null,
          "allowText": true
        }
      },
      "github": {
        "type": "Link",
        "config": {
          "label": "Github",
          "placeholder": "GitHub profile URL",
          "allowTargetBlank": true,
          "select": null,
          "allowText": true
        }
      },
      "talk": {
        "type": "Link",
        "config": {
          "label": "Talk",
          "placeholder": "",
          "select": "media",
          "allowText": true
        }
      }
    }
  }
}
